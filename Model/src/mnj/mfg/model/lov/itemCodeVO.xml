  <SQLQuery>
    <![CDATA[SELECT ooha.header_id,
       oola.line_id,
       ooha.cust_po_number,
       oola.inventory_item_id,
       mmpl.header_id POc_header_id,
       mmpl.line_id POC_line_id,
       ooha.SHIPPING_INSTRUCTIONS,
       mmph.DIVISION,
       oola.ATTRIBUTE6 color,
        oola.ATTRIBUTE7 wash
  FROM oe_order_lines_all oola,
       oe_order_headers_all ooha,
       mnj_mfg_precosting_h mmph,
       mnj_mfg_precosting_l mmpl
       
 WHERE     oola.header_id = ooha.header_id
            and mmph.header_id=mmpl.header_id
       AND TRIM (ooha.cust_po_number) = TRIM (mmpl.BPO_NO)
       and ooha.header_id= mmpl.sale_header_id
       -- and mmpl.header_id='17921'
      -- and mmpl.line_id='17315'
       and oola.line_id =mmpl.sale_line_id]]>
  </SQLQuery>
