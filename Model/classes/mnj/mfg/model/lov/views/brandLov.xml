<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="brandLov"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.mfg.model.lov.views.brandLov_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_customer"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <Variable
    Name="p_brand"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select qr.character1  customer_name,
               qr.character5  brand,
               ac.CUSTOMER_ID
          from qa_results qr, qa_plans qp, ar_customers ac
         where qr.plan_id = qp.plan_id
           and ac.CUSTOMER_NAME = qr.character1
           and qp.name = 'BUYER WISE DETAILS OM'
           and trunc(sysdate) between to_date(qr.character6, 'YYYY-MM-DD') and
               nvl(to_date(qr.character7, 'YYYY-MM-DD'), trunc(sysdate))]]>
  </SQLQuery>
  <ViewAttribute
    Name="CustomerName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CUSTOMER_NAME"
    Expression="CUSTOMER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Brand"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="150"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BRAND"
    Expression="BRAND"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CustomerId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="15"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="CUSTOMER_ID"
    Expression="CUSTOMER_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="16"/>
    </DesignTime>
  </ViewAttribute>
  <ViewCriteria
    Name="brandLovCriteria"
    ViewObjectName="mnj.mfg.model.lov.views.brandLov"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="false"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow95"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="CustomerName"
        ViewAttribute="CustomerName"
        Operator="="
        Conjunction="AND"
        Value=":p_customer"
        IsBindVarValue="true"
        Required="Optional"/>
      <ViewCriteriaItem
        Name="brandLovCriteria_vcrow95_Brand"
        ViewAttribute="Brand"
        Operator="="
        Conjunction="AND"
        Value=":p_brand"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.mfg.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
