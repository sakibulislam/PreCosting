  <SQLQuery>
    <![CDATA[select h.percosting_id,
       h.percosting_number,
       h.buyer_id,
       h.season,
       h.drop_period,
       h.brand,
       h.style,
       h.style_refer_no,
       h.fit,
       h.end_user,
       h.item_desc,
       sum(l.quantity) quantity,
       ac.CUSTOMER_NAME

  from CUST_MNJ_ONT_SAMPLE_HD    h,
       CUST_MNJ_ONT_SAMPLE_LINES l,
       ar_customers              ac
 where h.percosting_id = l.percosting_id
   and h.buyer_id = ac.CUSTOMER_ID(+)
 group by h.percosting_id,
          h.percosting_number,
          h.buyer_id,
          h.season,
          h.drop_period,
          h.brand,
          h.style,
          h.style_refer_no,
          h.fit,
          h.end_user,
          h.item_desc,
          ac.CUSTOMER_NAME]]>
