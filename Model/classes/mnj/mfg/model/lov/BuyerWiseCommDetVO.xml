<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BuyerWiseCommDetVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.mfg.model.lov.BuyerWiseCommDetVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select qr.character2 buyer_id,qr.character3 comm 
from qa_results qr
where qr.plan_id = 23339
and trunc(sysdate)  between nvl(to_date(qr.character4, 'YYYY/MM/DD'),trunc(sysdate)) and nvl(to_date(qr.character5, 'YYYY/MM/DD'),trunc(sysdate))]]>
  </SQLQuery>
  <ViewAttribute
    Name="BuyerId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BUYER_ID"
    Expression="BUYER_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Comm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="150"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COMM"
    Expression="COMM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.mfg.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
